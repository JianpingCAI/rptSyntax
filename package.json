{
  "name": "rpt-syntax",
  "displayName": "rpt Syntax",
  "description": "Syntax highlighting for .rpt file.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "rpt",
        "aliases": [
          "rpt",
          "RPT"
        ],
        "extensions": [
          ".rpt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rpt",
        "scopeName": "source.rpt",
        "path": "./syntaxes/rpt.tmLanguage.json"
      }
    ],
    "configurationDefaults": {
      "[rpt-syntax]": {
        "editor.tabSize": 8
      }
    }
  }
}